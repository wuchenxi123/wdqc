<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.manage.card.persistent.CardVO" table="ms_slot_card">
<id column="sc_id" name="scId" type="java.lang.Long">
<generator class="assigned"/>
</id>
<property column="st_id" length="10" name="stId" type="java.lang.Integer"/>
<property column="sc_type" length="3" name="scType" type="java.lang.String"/>
<property column="sc_date" name="scDate" type="java.util.Date"/>
<property column="sc_time" name="scTime" not-null="true" type="java.util.Date"/>
<property column="sc_week" length="5" name="scWeek" type="java.lang.Short"/>
<property column="sc_interval" name="scInterval" type="java.util.Date"/>
</class>
</hibernate-mapping>
