<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.manage.campus.persistent.CampusVO" table="ms_campus">
<id column="cp_Id" name="cpId" type="java.lang.Long">
<generator class="assigned"/>
</id>
<property column="cp_name" length="20" name="cpName" type="java.lang.String"/>
<property column="cp_recordDate" name="cpRecorddate" type="java.util.Date"/>
</class>
</hibernate-mapping>
