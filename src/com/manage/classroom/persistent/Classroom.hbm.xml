<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.manage.classroom.persistent.ClassroomVO" table="ms_classroom">
<id column="cr_id" name="crId" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="cr_name" length="20" name="crName" type="java.lang.String"/>
<property column="cr_capacity" length="5" name="crCapacity" type="java.lang.Short"/>
<property column="cr_campus" length="3" name="crCampus" type="java.lang.String"/>
<property column="ms_start_date" name="msStartDate" type="java.util.Date"/>
<property column="ms_end_date" name="msEndDate" type="java.util.Date"/>
</class>
</hibernate-mapping>
